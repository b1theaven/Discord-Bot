{
    "extends": "tslint:recommended",
    "rulesDirectory": [],
    "rules": {
        "class-name": false,
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": {
            "severity": "warning",
            "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
        },
        "max-classes-per-file": "off"
    },
    "jsRules": {
    }
}